create table uploads(
    class_id int NOT NULL,
    upload_id int NOT NULL auto_increment,
    tag_name_1 varchar(20) NOT NULL,
    tag_name_3 varchar(20),
    tag_name_2 varchar(20),
    tag_name_4 varchar(20),
    tag_name_5 varchar(20),
    tag_name_6 varchar(20),
    tag_name_7 varchar(20),
    tag_name_8 varchar(20),
    tag_name_9 varchar(20),
    tag_name_10 varchar(20),
    tag_name_11 varchar(20),
    tag_name_12 varchar(20),
    tag_name_13 varchar(20),
    tag_name_14 varchar(20),
    tag_name_15 varchar(20),
    PRIMARY KEY (upload_id),
    FOREIGN KEY (class_id) REFERENCES class(class_id),
    FOREIGN KEY (tag_name_1) REFERENCES tag_name(tag_name),
    FOREIGN KEY (tag_name_2) REFERENCES tag_name(tag_name),
    FOREIGN KEY (tag_name_3) REFERENCES tag_name(tag_name),
    FOREIGN KEY (tag_name_4) REFERENCES tag_name(tag_name),
    FOREIGN KEY (tag_name_5) REFERENCES tag_name(tag_name),
    FOREIGN KEY (tag_name_6) REFERENCES tag_name(tag_name),
    FOREIGN KEY (tag_name_7) REFERENCES tag_name(tag_name),
    FOREIGN KEY (tag_name_8) REFERENCES tag_name(tag_name),
    FOREIGN KEY (tag_name_9) REFERENCES tag_name(tag_name),
    FOREIGN KEY (tag_name_10) REFERENCES tag_name(tag_name),
    FOREIGN KEY (tag_name_11) REFERENCES tag_name(tag_name),
    FOREIGN KEY (tag_name_12) REFERENCES tag_name(tag_name),
    FOREIGN KEY (tag_name_13) REFERENCES tag_name(tag_name),
    FOREIGN KEY (tag_name_14) REFERENCES tag_name(tag_name),
    FOREIGN KEY (tag_name_15) REFERENCES tag_name(tag_name)
);